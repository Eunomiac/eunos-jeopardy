{
  "name": "eunos-jeopardy",
  "private": true,
  "version": "0.6.0",
  "type": "module",
  "description": "Online platform to create and host custom Jeopardy!-style games with friends. Features host-authored boards, buzzer lock/unlock, score tracking, Double and Final Jeopardy, and a polished, authentic interface.",
  "author": "Eunomiac",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "npm run lint && npm run test:ci && npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:ci-keep": "jest --coverage --watchAll=false --passWithNoTests && pause",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@sentry/react": "^10.10.0",
    "@supabase/supabase-js": "^2.50.0",
    "gsap": "^3.13.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@jest/globals": "^30.0.0-beta.3",
    "@playwright/test": "^1.55.0",
    "@sentry/vite-plugin": "^4.3.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.37.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-sonar-reporter": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "sass": "^1.89.0",
    "ts-jest": "^29.3.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^6.3.5"
  }
}
